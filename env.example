# Alinea SDK Configuration
# Copy this file to .env and set your actual values
# NEVER commit the .env file to version control!

# Your Alinea-AI backend URL
ALINEA_BASE_URL=http://localhost:8000

# Your API key (generate with your backend's auth system)
# Format: alinea_sk_[32_random_characters]
ALINEA_API_KEY=alinea_sk_your_secure_api_key_here

# Optional: Enable debug logging
ALINEA_DEBUG=false

# Optional: Request timeout in seconds
ALINEA_TIMEOUT=30